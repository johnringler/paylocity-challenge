{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "browser": true,
    "element": true,
    "by": true,
    "expect": true,
    "afterAll": true,
    "beforeAll": true,
    "protractor": true,
    "angular": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "comma-dangle": [0, "always-multiline"],
    "func-names": 0,
    "no-multi-spaces": 0,
    "no-underscore-dangle": 0,
    "key-spacing": [2, { "align": "value" }],
    "strict": 0,
    "new-cap": 0,
    "padded-blocks": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "no-console": 0,
    "arrow-body-style": [0, "as-needed"],
    "quote-props": [0, "consistent"],
    "no-useless-escape": 0
  }
}
